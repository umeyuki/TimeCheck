use strict;
use warnings;
use Time::Check;
use Test::More tests => 3;
use_ok('Time::Check');                
my $time = new Time::Check(1,5);
can_ok('Time::Check', 'check');
is($time->check(3),1);
is($time->check(3),1);
is($time->check(3),1);
is($time->check(3),1);
is($time->check(3),1);
is($time->check(3),1);

undef $time;
